#include<iostream>
#include<cstring>
#include<string>
#include<vector>
using namespace std;
vector<int> getNext(string s) {
	vector<int> next(s.size());
	int j = 0;
	int k = -1;
	next[0] = -1;
	while (j < s.size() - 1) {
		if (k==-1 || s[j]==s[k])
		{
			next[++j] = ++k;
		}
		else {
			k=next[k];
		}
	}
	return next;
}

int KMP(string s,string t) {
	vector<int> next = getNext(s);
	int n = s.size();
	int m = t.size();
	int i = 0;
	int j = -1;
	while (i<n && j<m)
	{
		if (j==-1 || s[i]==t[j])
		{
			i++;
			j++;
		}
		else {
			j = next[j];
		}
	}
	if (j==m)
	{
		return i - j;
	}
	else {
		return -1;
	}
}
int main() {
	string s = "abcdefg";
	string t = "deg";
	cout << KMP(s, t) << endl;
	return 0;
}
